<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Name</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
        <td><%= event.name %></td>
        <td><%= event.description %></td>
        <td><%= link_to 'Show', [@user, event] %></td>
        <td><%= link_to 'Edit', edit_user_event_path(@user, event) %></td>
        <td><%= link_to 'Destroy', [@user, event], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_user_event_path %>
